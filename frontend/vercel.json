{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      },
      "rewrites": [
        { "source": "/serviceInquiry", "destination": "/" },
        { "source": "/about", "destination": "/" },
        { "source": "/gallery", "destination": "/" },
        { "source": "/comingSoon", "destination": "/" },
        { "source": "/winder", "destination": "/" },
        { "source": "/printHub", "destination": "/" }
      ]
    }
  ]
}